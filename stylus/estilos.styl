	.videos
		width 95%
.imgiz
	width 95%

//Icon
@font-face
	font-family: 'icomoon'
	src:url('../fonts/icomoon.eot?zglzvn')
	src:url('../fonts/icomoon.eot?#iefixzglzvn') format('embedded-opentype'),
		url('../fonts/icomoon.woff?zglzvn') format('woff'),
		url('../fonts/icomoon.ttf?zglzvn') format('truetype'),
		url('../fonts/icomoon.svg?zglzvn#icomoon') format('svg')
	font-weight: normal
	font-style: normal

[class^="icon-"], [class*=" icon-"]
	font-family: 'icomoon'
	speak: none
	font-style: normal
	font-weight: normal
	font-variant: normal
	text-transform: none
	line-height: 1

	-webkit-font-smoothing: antialiased
	-moz-osx-font-smoothing: grayscale


.icon-home:before
	content: "\e600"

.icon-profile:before
	content: "\e601"

.icon-play:before
	content: "\e602"
